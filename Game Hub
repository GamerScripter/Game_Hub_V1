game:GetService("StarterGui"):SetCore("SendNotification",{
 Title = "Game Hub Test Loaded",
 Text = "Made by Gamer Scripter",
 Icon = "",
 Duration = "3"
})
local function callback(Text)
 if Text == "Copy Link" then
 setclipboard("Try")
 elseif Text == ("Continue") then
 print("Welcome")
 end
end
local NotificationBindable = Instance.new("BindableFunction")
NotificationBindable.OnInvoke = callback
--
game.StarterGui:SetCore("SendNotification",{
 Title = "Welcome";
 Text = "Sub To My YT Channel";
 Icon = "";
 Duration = 10;
 Button1 = "Copy Link";
 Button2 = "Continue";
 Callback = NotificationBindable;
})
local colors = {
    SchemeColor = Color3.fromRGB(16, 88, 233),
	Background = Color3.fromRGB(10, 10, 10),
    Header = Color3.fromRGB(5, 5, 5),
    TextColor = Color3.fromRGB(255,255,255),
    ElementColor = Color3.fromRGB(20, 20, 20)
}
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/YellowGreg/AFGui/main/GUI"))()
local Window = Library.CreateLib("Game Hub ðŸ¤¡ | By: Gamer Scripter ", colors)
--Home
local Home = Window:NewTab("Home")
local HomeSection = Home:NewSection("Player")
HomeSection:NewSlider("Walk Speed", "Changes Walk Speed", 800, 16, function(v)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
end)
HomeSection:NewSlider("Jump Power", "Changes Jump Power", 800, 50, function(v)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = v
end)
HomeSection:NewButton("Reset WS/JP", "Reset walk speed and jump power", function()
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
end)
HomeSection:NewButton("Inf Jump", "Enable Inf Jump", function()
    local InfiniteJumpEnabled = true
game:GetService("UserInputService").JumpRequest:connect(function()
	if InfiniteJumpEnabled then
		game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")
	end
end)
end)
HomeSection:NewToggle("Fov", "Changes Fov", function(state)
    if state then
        game.Workspace.CurrentCamera.FieldOfView = 130
    else
        game.Workspace.CurrentCamera.FieldOfView = 80
    end
end)
HomeSection:NewButton("No Clip (Press E To Use)","Pass through Wall(Use Keyboard Script On Mobile)",function()
local StealthMode = true

local Indicator

if not StealthMode then
    local ScreenGui = Instance.new("ScreenGui", game.CoreGui)
    print("NOCLIP: Press E to Activate")
    Indicator = Instance.new("TextLabel", ScreenGui)
    Indicator.AnchorPoint = Vector2.new(0, 1)
    Indicator.Position = UDim2.new(0, 0, 1, 0)
    Indicator.Size = UDim2.new(0, 200, 0, 50)
    Indicator.BackgroundTransparency = 1
    Indicator.TextScaled = true
    Indicator.TextStrokeTransparency = 0
    Indicator.TextColor3 = Color3.new(0, 0, 0)
    Indicator.TextStrokeColor3 = Color3.new(1, 1, 1)
    Indicator.Text = "Noclip: Enabled"
end

local noclip = false
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()

local mouse = player:GetMouse()

mouse.KeyDown:Connect(function(key)
    if key == "e" then
        noclip = not noclip

        if not StealthMode then
            Indicator.Text = "Noclip: " .. (noclip and "Enabled" or "Disabled")
        end
    end
end)

while true do
    player = game.Players.LocalPlayer
    character = player.Character

    if noclip then
        for _, v in pairs(character:GetDescendants()) do
            pcall(function()
                if v:IsA("BasePart") then
                    v.CanCollide = false
                end
            end)
        end
    end

    game:GetService("RunService").Stepped:wait()
end
end)
